<?xml version="1.0" ?>

<launch>
  
  <param name="table_description" textfile="$(find scene)/urdf/table.urdf"/>
     
  <node name="table_spawner" pkg="gazebo_ros" type="spawn_model" output="screen"
      args="-param table_description -urdf -model table -x 0 -y 0.71 -z 0.6 -R 0 -P 0 -Y 1.5708"/>
      
  <param name="puck_description" textfile="$(find scene)/urdf/puck.urdf"/>
 
 <node name="puck_spawner" pkg="gazebo_ros" type="spawn_model" output="screen"
      args="-param puck_description -urdf -model puck -x 0 -y 0.3 -z 1"/>
      
 <param name="camera_description" textfile="$(find scene)/urdf/camera.urdf"/>
 
 <node name="camera_spawner" pkg="gazebo_ros" type="spawn_model" output="screen"
      args="-param camera_description -urdf -model camera -x 0 -y 0.4 -z 1.25"/>
      
</launch>
