<?xml version="1.0" ?>

<launch>
  
   
  <param name="scene_description" textfile="$(find scene)/sdf/scene.sdf"/>
  <node name="scene_spawner" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-param scene_description -sdf -model scene"/>
        
   <param name="puck_description" textfile="$(find scene)/sdf/puck/model.sdf"/>
  <node name="puck_spawner" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-param puck_description -sdf -model puck -x 0 -y 0.3 -z 1"/>
        
   <param name="camera_description" textfile="$(find scene)/sdf/camera/model.sdf"/>
  <node name="camera_spawner" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-param camera_description -sdf -model camera -x 0 -y 0 -z 1.6"/>
        

    
</launch>
